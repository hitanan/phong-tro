{
	"name": "tìm phòng, laptop cũ",
	"author": "An Nguyen",
	"description": "tìm phòng trọ, laptop cũ tại Đà Nẵng Việt Nam, trên chotot.com, ..",
	"version": "1.0",
	"permissions": ["contextMenus", "tabs", "clipboardRead", "storage"],
	"icons": {
		"16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
	},
	"background": { "scripts": ["background.js"],"persistent": false },
	"browser_action": {
	   "default_icon": {
			"19": "icon.png",
			"38": "icon.png"
		},
		"default_title": "open link",
		"default_popup": "brower_action.html"
	},
	"options_page": "options.html",
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"content_scripts": [
		{
		  "matches": ["https://www.chotot.com/quang-nam-da-nang/thue-phong-tro*", "https://www.chotot.com/quang-nam-da-nang/thue-nha-dat*", "https://www.chotot.com/quang-nam-da-nang/thue-van-phong-mat-bang-kinh-doanh*"],
		  "js": ["jquery.min.js", "sites/chotot.com/common.js", "sites/chotot.com/index.js"]
		}, {
		  "matches": ["https://www.chotot.com/quang-nam-da-nang/mua-ban-may-tinh-laptop*",
		  "https://m.chotot.com/quang-nam-da-nang/mua-ban-may-tinh-laptop*", "https://www.chotot.com/quang-nam-da-nang/da-nang/mua-ban-may-tinh-laptop*", "https://www.chotot.com/tp-ho-chi-minh/quan-1/mua-ban-may-tinh-laptop*"],
		  "js": ["jquery.min.js", "sites/chotot.com/common.js", "sites/chotot.com/laptop.js"]
		}
	],
	"web_accessible_resources": [ "/*"],
	"manifest_version": 2
}
